}
1005:
#include <stdio.h>
 
int main() {
 
   double A, B, res;
   scanf("%lf",&A);
   scanf("%lf",&B);
   res= ((A*3.5) + (B*7.5))/11;
   printf("MEDIA = %.5lf\n", res);
    return 0;
}
1006:
include <stdio.h>
 
int main() {
 
    double A, B, C, res;
    scanf("%lf",&A);
    scanf("%lf",&B);
    scanf("%lf",&C);
    res=((A*2) + (B*3) + (C*5))/10;
    printf("MEDIA = %.1lf\n",res);
    return 0;
}
1007:
#include <stdio.h>
 
int main() {
 
   int A, B, C, D, res;
       scanf("%d", &A);
       scanf("%d", &B);
       scanf("%d", &C);
       scanf("%d", &D);
       res= A*B-C*D;
       printf("DIFERENCA = %d\n", res);
 
    return 0;
}
1008:
#include <stdio.h>
 
int main() {
 
    int n, d;
    float h, s;
   
       scanf("%d", &n);
       scanf("%d", &d);
       scanf("%f", &h);
       s = d*h;
       printf("NUMBER = %d\nSALARY = U$ %.2f\n",n,s);
       
      
 
    return 0;
}
